/* pages/home/home.wxss */

.m-home-banner {
    width: 100%;
    height: 600rpx;
}

.m-home-banner image {
    width: 750rpx;
    height: 600rpx;
}
swiper {
    width: 100%;
    height: 100%;
}

swiper .wx-swiper-dot {
    width: 20rpx;
    height: 20rpx;
    background-color: #fff;
    background:url('data:image/png;base64,') no-repeat center/cover;
}

swiper .wx-swiper-dot-active {
    background-image:url('data:image/png;base64,');
    border-radius: 0;
}

.m-topColumn, .topNav {
    box-sizing: border-box;
    width: 100%;
}

.topNav{
    padding-top: 28px;
    padding-left: 50rpx;
    padding-right: 50rpx;
}

.m-topColumn .topNav image{
    width: 180rpx;
    height: 180rpx;
}

.m-topColumn .topNav .aLink {
    padding-top: 23rpx;
    width: 1em;
    font-size: 22rpx;
    color: #000;
}

.m-subColumn {
    padding-top: 100rpx;
    width: 100%;
}

.m-subColumn .column_title {
    box-sizing: border-box;
    padding-top: 23rpx;
    padding-bottom: 23rpx; 
    width: 600rpx;
    height: 154rpx;
}

.m-subColumn .column_title .title {
    font-size: 30rpx;
    line-height: 36rpx;
    color: #000;
}

.m-subColumn .column_title .nickname {
    font-size: 14rpx;
    line-height: 14rpx;
    color: #000;
}

.m-subColumn .column_title image {
    width: 30rpx;
    height: 30rpx;
}

.m-subColumn .columnHead .banner image {
    width: 100%;
    height: 306rpx;
}

.m-columnItem-fir {
    padding: 50rpx 50rpx 0;
}

.m-columnItem-fir .goods {
    padding-top: 5rpx;
    padding-bottom: 9rpx;
}

.m-columnItem-fir image {
    width: 300rpx;
    height: 300rpx;
}

.m-columnItem-fir .name {
    padding-top: 25rpx;
    padding-right: 21rpx;
    width: 100%;
    font-size: 24rpx;
    line-height: 33rpx;
    text-align: right;
}

.m-columnItem-fir .nickname {
    font-size: 18rpx;
    padding-right: 21rpx;
    width: 100%;
    line-height: 25rpx;
    text-align: right;
}

.m-columnItem-sec image {
    width: 400rpx;
    height: 400rpx;
}

.m-columnItem-sec .goods {
    padding-top: 30rpx;
}

.m-columnItem-sec .f-flex {
    padding-left: 35rpx; 
}

.m-columnItem-sec .f-flex-reverse {
    padding-right: 35rpx;
}

.m-columnItem-sec .name {
    font-size: 24rpx;
    line-height: 37rpx;
}

.m-columnItem-sec .nickname {
    font-size: 18rpx;
    line-height: 25rpx;
}

.m-columnItem-thd {
    padding: 30rpx 50rpx;
}

.m-columnItem-thd image {
    width: 325rpx;
    height: 200rpx;
}

.m-columnItem-thd .name {
    box-sizing: border-box;
    width: 100%;
    font-size: 30rpx;
    line-height: 48rpx;
}

.m-columnItem-thd .nickname {
    box-sizing: border-box;
    width: 100%;
    font-size: 18rpx;
    line-height: 34rpx;
}

.m-columnItem-thd .f-flex-reverse .name {
    padding-right: 75rpx;
    text-align: right;
}

.m-columnItem-thd .f-flex-reverse .nickname {
    padding-right: 75rpx;
    text-align: right;
}

.m-columnItem-thd .f-flex .name {
    padding-left: 75rpx;
    text-align: left;
}

.m-columnItem-thd .f-flex .nickname {
    padding-left: 75rpx;
    text-align: left;
}


