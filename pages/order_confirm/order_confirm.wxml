<!--pages/order_confirm/order_confirm.wxml-->
<view class="container">
    <view class='address-confirm f-flex'>
        <image
            src='data:image/png;base64,'
        ></image>
        <view class='address-txt f-flex-fixed f-flex-col-cs'>
            <text>浙江省-|-杭州市-|-下城区</text>
            <text>蓝图创客基地6幢</text>
            <text>收件人: 跳跳糖 13566666666</text>
        </view>
        <image class='link'
            src='data:image/png;base64,'
        ></image>
    </view>
    <view class='m-goodList-row'>
        <block wx:for='{{orderInfo.list}}' wx:key='{{index}}'>
            <view class='cart-item f-cb'>
                <view class='hold f-flex' style="{{item.txtStyle}}" data-index='{{index}}' bindtouchstart="touchS" bindtouchmove="touchM" bindtouchend="touchE">
                    <image src='{{item.imgUrl}}' class='goodsImg'>
                    </image>
                    <view class='info f-flex-fixed'>
                        <view class='f-flex-wrap'>
                            <text>{{item.name}}</text>
                            <text class='color'>￥{{item.price}}</text>
                        </view>
                        <text class='amount color f-flex-end'>X{{item.amount}}</text>
                        <view class='norm'>
                            <block wx:for='{{item.norm}}' wx:key='{{index}}'>
                                <text>{{item}}</text>
                            </block>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </view>
</view> 
