/* pages/goods_detail/goods_detail.wxss */
 
.container {
    padding-bottom: 100rpx;
}

.m-swiper {
    padding-top: 32rpx;
    padding-bottom: 50rpx;
    width: 100%;
    height: 632rpx;
}

.m-swiper swiper {
    width: 100%;
    height: 100%;
    background-color: rgba(255,255,255,0.1);
}

.m-swiper swiper swiper-item {
    left: 58rpx;
    bottom: 0;
    padding-left: 15rpx;
    padding-right: 15rpx;
    width: 600rpx !important;
    height: 600rpx !important;
    transition: bottom .3s ease-in-out; 
}

.m-swiper swiper .swiper-active {
    bottom: 30rpx;
}

.m-swiper swiper-item image{
    width: 100%;
    height: 100%;
    border-radius: 8px;
}

swiper .wx-swiper-dots {
    bottom: 0;
}

swiper .wx-swiper-dot {
    width: 20rpx;
    height: 20rpx;
    background-color: #fff;
    background: url('data:image/png;base64,') no-repeat center/cover;
}

swiper .wx-swiper-dot-active {
    background-image:url('data:image/png;base64,');
    border-radius: 0;
}

.m-detailHtml image {
    width: 750rpx;
    height: 200rpx;
}
.m-goodsInfo {
    padding-left: 70rpx;
    padding-right: 70rpx;
}

.m-goodsInfo .title {
    padding-left: 3rpx;
    height: 95rpx;
}

.m-goodsInfo .title view {
    display: inline-block;
    /* margin-top: 30rpx; */
    width: 5rpx;
    height: 35rpx;
    vertical-align: middle;
    background-color: #000;
}

.m-goodsInfo .title text {
    padding-left: 25rpx;
    font-size: 30rpx;
    line-height: 95rpx;
    font-weight: bold;
}

.m-goodsInfo .info {
    display: inline-block;
    padding-left: 32rpx;
    padding-right: 32rpx;
    font-size: 23rpx;
    line-height: 43rpx;
    text-indent: 2em;
}

.m-goodsInfo .price {
    padding-left: 32rpx;
    padding-right: 32rpx;
}

.m-goodsInfo .price i {
    display: inline-block;
    font-size: 22rpx;
}

.m-goodsInfo .price span {
    display: inline-block;
    font-size: 36rpx;
    line-height: 143rpx;
}

.m-detail-bottomBar {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
}

.m-detail-bottomBar .link {
    padding-left: 32rpx;
    padding-right: 32rpx;
    font-size: 28rpx;
    line-height: 62rpx;
    color: #648749;
    border: 1px solid #648749;
    border-radius: 28rpx/24rpx;
}

.m-detail-bottomBar .doCart {
    padding-left: 32rpx;
    padding-right: 30rpx;
    font-size: 28rpx;
    line-height: 62rpx;
    color: #fff;
    background-color: #648749;
    border: 1px solid #648749;
    border-radius: 28rpx/24rpx;
}

.m-detail-paramselect {
    position: fixed;
    z-index: 10;
    left:0;
    bottom: -940rpx;
    width: 100%;
    transition: bottom .3s cubic-bezier(0.075, 0.82, 0.165, 1)
}

.active-popup {
    z-index: 501;
    bottom: 0;
}

.m-detail-paramselect .confirm {
    width: 100%;
    height: 98rpx;
    font-size: 34rpx;
    line-height: 98rpx;
    text-align: center;
    color: #fff;
    background-color: #648749;
}

.m-detail-paramselect .paramselect {
    padding: 23rpx 19rpx 0;
    background-color: #fff;
    border: 1px solid #648749;

}

.m-detail-paramselect .paramselect .info {
    padding-bottom: 34rpx;
}

.m-detail-paramselect .paramselect .info image {
    margin: 8rpx 50rpx 0 30rpx;
    width: 200rpx;
    height: 200rpx;
}

.m-detail-paramselect .paramselect .info .name, .m-detail-paramselect .paramselect .info .secName {
    width: 100%;
}

.m-detail-paramselect .paramselect .info .name {
    font-size: 36rpx;
    line-height: 56rpx;
    color: #333;
}

.m-detail-paramselect .paramselect .info .secName {
    font-size: 28rpx;
    line-height: 48rpx;
    color: #333;
}

.m-detail-paramselect .paramselect .info .hold {
    margin-top: 70rpx;
    width: 100%;
}

.m-detail-paramselect .paramselect .info .hold .discountPrice {
    font-size: 48rpx;
    line-height: 48rpx;
}

.m-detail-paramselect .paramselect .info .hold .price {
    margin-left: 15rpx;
    font-size: 24rpx;
    color: #333;
    text-decoration: line-through;
}

.m-detail-paramselect .paramselect .tetChoose text {
    line-height: 58rpx;
}

.m-detail-paramselect .paramselect .tetChoose .fs-23 {
    padding-left: 33rpx;
}

.m-detail-paramselect .paramselect .chooseList {
    padding-top: 15rpx;
    border-bottom: 1px solid #648749;
}

.m-detail-paramselect .paramselect .chooseList view {
    padding-left: 45rpx;
    padding-right: 45rpx;
    margin-bottom: 30rpx;
    margin-right: 16rpx;
    line-height: 68rpx;
    border: 1px solid #648749;
    border-radius: 10rpx;

}

.m-detail-paramselect .paramselect .amount {
    border-bottom: 1px solid #8a8a8a;
}

.m-detail-paramselect .paramselect .amount .selNum {
    margin-right: 50rpx;
}

.m-detail-paramselect .paramselect .amount text {
    line-height: 100rpx;
}

.m-detail-paramselect .paramselect .amount .selNum {
    border: 1px solid #648749;
    border-radius: 10rpx;
}

.m-detail-paramselect .paramselect .amount .num {
    padding-left: 50rpx;
    padding-right: 50rpx;
    height: 58rpx;
    border-left: 1px solid #648749;
    border-right: 1px solid #648749;
    font-size: 22rpx;
    line-height: 58rpx;
}

.m-detail-paramselect .paramselect .amount .btn {
    width: 89rpx;
    height: 58rpx;
    font-size: 24rpx;
    line-height: 58rpx;
    text-align: center;
}

.m-detail-paramselect .paramselect .amount .add {
    background-color: #648749;
    color: #fff;
}

.m-detail-paramselect .paramselect .recTime {
    padding-top: 30rpx;
    padding-bottom: 20rpx;
}

.m-detail-paramselect .paramselect .recTime text {
    height: 43rpx;
    line-height: 43rpx;
}

.paramselect .recTime .period view {
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin-left: 33rpx;
    margin-bottom: 16rpx;
    height: 58rpx;
    font-size: 24rpx;
    line-height: 58rpx;
}

.popup-mask {
    display: block;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    opacity: 0;
    tap-highlight-color: rgba(0, 0, 0, 0);
    z-index: -1;
    transition: opacity 400ms;
}

.mask-show {
    z-index: 500;
    opacity: 1;
}

.m-detail-paramselect .paramselect .active-btn {
    color: #fff;
    background-color: #648749;
}

