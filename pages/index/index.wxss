/**index.wxss**/

.f-position-rel {
    position: relative;
}

.f-flex {
    display: flex;
    flex-flow: row nowrap;
} 

.f-flex-fixed {
    flex: 1;
}

.f-flex image {
    flex: 1;
}

.f-flex-cc {
    display: flex;
    justify-content: center;
    align-items: center;
}

.m-indexFloor {
    width: 750rpx;
    height: 500rpx;
}

.m-indexFloor image {
    width: 100%;
    height: 500rpx;
}

swiper {
    width: 100%;
    height: 100%;
}

swiper .wx-swiper-dot {
    width: 20rpx;
    height: 20rpx;
    background-color: #fff;
    background: url('data:image/png;base64,') no-repeat center/cover;
}

swiper .wx-swiper-dot-active {
    background-image:url('data:image/png;base64,');
    border-radius: 0;
}

.m-shop-title {
    width: 250rpx;
    height: 500rpx;
    background-color: #648749;
}

.m-shop-title .content {
    width: 150rpx;
}

.content p {
    display: block;
    width: 100%;
    font-size: 48rpx;
    line-height: 63rpx;
    font-family: "Arial Bold"; 
    color: #fff;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
}

.content i {
    font-family: "PingFang SC";
    font-size: 36rpx;
    line-height: 58rpx;
    color: #fff;
}

.m-gift-title {
    position: absolute;
    right: 0;
    bottom: 0;
    width: 250rpx;
    height: 250rpx;
    background-color: #648749;
}

.m-gift-title .content {
    width: 115rpx;
}

.m-gift-title .content p {
    line-height: 53rpx;
}

.m-gift-title .content i {
    padding-top: 20rpx;
    line-height: 36rpx;
}
