<!--pages/order_detail/order_detail.wxml-->
<view class="container">
    <text class='m-title-states fs-26'>{{states[2]}}</text>
    <view class='m-address blank'>
        <view class='address f-flex'>
            <image src='data:image/png;base64,'></image>
            <view class='address-txt f-flex-fixed f-flex-col-cs fs-23'>
                <text class='text-ell'>浙江省杭州市下城区蓝图创客基地6幢</text>
                <text class='text-ell'>收件人: 跳跳糖 13566666666</text>
            </view>
        </view>
    </view>
    <view class='goods-info'>
        <block wx:for='{{request.goodsInfo}}'>
            <view class='hold f-flex'>
                <image src='{{item.imgUrl}}' class='goodsImg'>
                </image>
                <view class='info f-flex-fixed'>
                    <view class='f-flex-wrap'>
                        <text class='fs-23' style='line-height: 47rpx;'>{{item.name}}</text>
                        <text class='fs-16' style='line-height: 47rpx;'>￥{{item.price}}</text>
                    </view>
                    <view class='norm f-flex-wrap'>
                        <text class='fs-16' style='line-height: 42rpx;color: #999;'>{{item.norm[0]}}</text>
                        <text class='fs-16' style='line-height: 42rpx;'>X{{item.amount}}</text>
                    </view>
                </view>
            </view>
        </block>
        <view class='additional fs-23'>
            <view class='fare f-flex-wrap'>
                <text>运费</text>
                <text>包邮</text>
            </view>
            <view class='discount f-flex-wrap'>
                <text>优惠</text>
                <text>￥-10</text>
            </view>
            <view class='total f-flex-wrap'>
                <text>合计</text>
                <text>￥188</text>
            </view>
        </view>
    </view>
    <view class='order-footer fs-23'>
        <view class='orderNum f-flex-fs-c'>
            <text>订单编号:</text>
            <text>1234567899000</text>
        </view>
        <view class='orderTime f-flex-fs-c'>
            <text>下单时间:</text>
            <text>2018年1月1日 14:01</text>
        </view>
        <view class='order-btn'>

        </view>
    </view>
</view>