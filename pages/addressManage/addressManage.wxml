<!--pages/addressManage/addressManage.wxml-->
<view class="container"> 
    <view class="address-list">
        <block wx:for="{{addressList}}" wx:key="{{index}}">
            <view class='hold'> 
                <view class='cell f-flex' style="{{item.txtStyle}}" data-index="{{index}}" bindtouchstart="touchS" bindtouchmove="touchM" bindtouchend="touchE">
                    <view class="hold f-flex-cc" data-addressid="{{item.id}}" bindtap='chooseAddress'>
                        <image style="width: 62rpx;height: 44rpx;" wx:if="{{item.isChecked==1}}" src='data:image/png;base64,'></image>
                    </view>
                    <view class='f-flex-col f-flex-fixed addressConcent' data-addressid="{{item.id}}" bindtap='reviseAddress'>
                        <text class='fs-34' style="line-height: 50rpx;">{{item.name}} {{item.phone}}</text>
                        <text class='fs-28 text-ell' style="width: 558rpx;line-height: 44rpx;color: #999;">杭州市{{item.area + item.detaile}}</text>
                    </view>
                </view>
                <view class='slipDelete' data-index='{{index}}' data-addressid='{{item.id}}' bindtap='delItem'>
                    删除
                </view>
            </view>
        </block>
    </view>
    <view class='address-append cell f-flex' data-page="appendAddress" bindtap='toLink'>
        <view class="hold f-flex-cc">
            <image style="width: 48rpx;height: 48rpx;" src='data:image/png;base64,'></image>
        </view>
        <view class='f-flex-col f-flex-fixed addressConcent'>
            <text class='fs-34' style="line-height: 95rpx;">新增地址</text>
        </view>
    </view>
</view>